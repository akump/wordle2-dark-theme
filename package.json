    "prettier": "prettier-airbnb-config",
    "type": "module",
    "dependencies": {
        "@types/chrome": "^0.0.133",
        "gulp": "^4.0.2",
        "gulp-zip": "^5.1.0",
        "sass": "^1.32.6"
    },
    "devDependencies": {
